    "\n",
    "#Average Purchase Price\n",
    "avg_price=purchase_data.groupby(['Age Ranges']).mean()['Price']\n",
    "avg_price\n",
    "\n",
    "#Total Per Person\n",
    "total_per_person=purchase_data.groupby(['Age Ranges']).sum()['Price']\n",
    "total_per_person\n",
    "\n",
    "#Average Total Purchase Per Person\n",
    "avg_total=total_per_person/players_age\n",
    "avg_total\n",
    "\n",
    "#Create a DataFrame to summarize findings\n",
    "purchasing_analysis_df=pd.DataFrame({\"Purchase Count\":age_count,\n",
    "                                    \"Average Price\":avg_price,\n",
    "                                    \"Total Purchase Value\":total_per_person,\n",
    "                                    \"Avg Total Purchase Per Person\":avg_total})\n",
    "\n",
